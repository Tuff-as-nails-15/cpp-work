#include<iostream>
using namespace std;

int main()
{
    //one statement 2 stream insertion operators
    cout << "Welcome to" << " C++ programming\n";

    //one statement 4 stream insertion operators
    cout << "Welcome" << " to" << " C++" << " programming\n";

    //two statements 2 steeam insertion operators one each
    cout << "Welcome to";
    cout << " C++ programming\n";
}
