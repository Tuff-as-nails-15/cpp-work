#include<iostream>
using namespace std;

int main()
//define variables
{
	int n = 1,counter=0;
	//create a counter for each table, and make the numbers double and indent.
	while (counter  <= 9)
	{
		cout << n << '\t' << n*2 << '\t' << n*4 << '\t' << n*8 << '\t' << '\n';
		n++;
		counter++;
	}
}
